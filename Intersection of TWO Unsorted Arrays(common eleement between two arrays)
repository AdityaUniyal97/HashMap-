public static void insertion(int arr[] , int brr[])
{
    int m = arr.length;
    int n = arr.length;
    HashSet<Integer> s = new HashSet<>();
    for(int num : brr)
    {
        s.add(num); // adding the element of brr array into the map s
    }
    
    for(int i =0 ; i < m ; i++)
    {
        if(s.contains(arr[i]))
        {
            System.out.println(arr[i]); 
        }
    }
}

// Printing the common element between the two arrays by adding the element of first array into map and iterating to the 2nd array if same element found than print it
